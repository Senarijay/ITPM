<style>



    .Sec{
        width:auto;
        height:300px;
    }
    .head1 {
        text-align: center;
        font-family: Bebas;
        font-size: 72px;
        background: -webkit-linear-gradient(#4800ff, #0094ff);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
    }
    .my-custom-scrollbar{
        position: relative;
        height: 700px;
        width: auto;
        overflow-x: auto;
    }

    .table-wrapper-scroll-y {
        display: inline-block;
    }
    .head2 {
        text-align: center;
        font-size: 25px;
    }

